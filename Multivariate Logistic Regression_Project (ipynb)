      "cell_type": "code",
      "source": [
        "#compute train confusion matrix\n",
        "tn, fp, fn, tp = confusion_matrix(x_train[\"target\"], x_train[\"prediction\"]).ravel()\n",
        "\n",
        "#calculate train sensitivity\n",
        "sensitivity = tp / (tp + fn)\n",
        "\n",
        "print(\"Sensitivity:\", sensitivity)"
      ],
